def tdan_dx_grp_vulvar_ca(raw):
    violation_mask = (
        (raw['tdan_dx_grp'] == 4) & (
            (raw['site_p'] != 7) |
            ((raw['site_p2'].notna()) & (raw['site_p2'] != 7)) |
            ((raw['site_p3'].notna()) & (raw['site_p3'] != 7))
        )

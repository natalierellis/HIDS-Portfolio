NCDB11 <- NCDB11 %>%
  mutate(TTC = DX_CHEMO_STARTED_DAYS - DX_DEFSURG_STARTED_DAYS, .before = DX_DEFSURG_STARTED_DAYS)
NCDB <- NCDB11 %>%
  mutate(DX_LASTCONTACT_DEATH_DAYS = round(DX_LASTCONTACT_DEATH_MONTHS * 30.417), .before = PUF_CASE_ID)
dim(NCDB)
NCDB <- NCDB %>%
  filter(DX_LASTCONTACT_DEATH_DAYS > DX_CHEMO_STARTED_DAYS | DX_LASTCONTACT_DEATH_DAYS > DX_DEFSURG_STARTED_DAYS)
dim(NCDB)

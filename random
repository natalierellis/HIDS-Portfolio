#check missingness for TDAN Summary Dx
def tdan_dx_missing(raw):
    violation_mask = (
        raw['tdandiagnosis_dx'].isna() |
        raw['tdan_dx_grp'].isna() |
        raw['site_p'].isna() |
        raw['tdan_hist_grp'].isna() |
        raw['grade_p'].isna() |
        raw['celltype_p'].isna() 
    )

        # Get rows that violate the rule
    violations = raw[violation_mask]

    return(violations)
